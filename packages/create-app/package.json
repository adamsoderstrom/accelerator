{
  "name": "@adamsoderstrom/create-app",
  "version": "0.5.0",
  "private": false,
  "author": "Adam Söderström",
  "description": "Create NoA Ignite react apps with one command",
  "keywords": [
    "react"
  ],
  "license": "MIT",
  "bin": {
    "create-app": "./index.js"
  },
  "files": [
    "index.js",
    "createApp.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/noaignite/accelerator.git",
    "directory": "packages/create-app"
  },
  "bugs": {
    "url": "https://github.com/noaignite/accelerator/issues"
  },
  "scripts": {
    "build": "pnpm prebuild && mkdir dist && cp -a package.json LICENSE CHANGELOG.md README.md createApp.js index.js dist/",
    "prebuild": "rm -rf dist",
    "release": "pnpm build && npm publish ./dist --tag latest",
    "release:yalc": "pnpm build && yalc publish ./dist"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.4.0",
    "cross-spawn": "^7.0.3",
    "fs-extra": "^10.1.0",
    "validate-npm-package-name": "^4.0.0"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
